{
    "name": "perspective_dash_component",
    "version": "0.0.4",
    "description": "A dash component for perspective.",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack-serve ./webpack.serve.config.js --open",
        "validate-init": "python3 _validate_init.py",
        "prepublish": "npm run validate-init",
        "build": "webpack --mode production",
        "build:js-dev": "webpack --mode development",
        "build:js": "webpack --mode production",
        "build:py": "node ./extract-meta.js src/lib/components > perspective_dash_component/metadata.json && copyfiles package.json perspective_dash_component && venv/bin/python -c \"import dash; dash.development.component_loader.generate_classes('perspective_dash_component', 'perspective_dash_component/metadata.json')\"",
        "build:all": "npm run build:js && npm run build:js-dev && npm run build:py"
    },
    "author": "Tim Paine t.paine154@gmail.com",
    "license": "Apache-2.0",
    "dependencies": {
        "@jpmorganchase/perspective": "^0.2.9",
        "@jpmorganchase/perspective-viewer": "^0.2.9",
        "@jpmorganchase/perspective-viewer-highcharts": "^0.2.9",
        "@jpmorganchase/perspective-viewer-hypergrid": "^0.2.9",
        "ramda": "^0.25.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "arraybuffer-loader": "^1.0.7",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.0",
        "copyfiles": "^2.0.0",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-react": "^7.9.1",
        "npm": "^6.1.0",
        "react": ">=0.14",
        "react-docgen": "^2.20.1",
        "react-dom": ">=0.14",
        "style-loader": "^0.21.0",
        "wasm-loader": "^1.3.0",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.1",
        "webpack-serve": "^1.0.2"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
